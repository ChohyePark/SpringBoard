<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="Members">
  <insert id="insert">
  	insert into member values (
  		member_seq.nextval,
  		#{id},
  		#{pw},
  		#{name},
  		#{contact},
  		#{email},
  		#{zipcode},
  		#{address1},
  		#{address2}
  		) 
  </insert>
  
  <update id = "update">
	update member set id = #{id},
					contact = #{contact}, 
					email = #{email}, 
					zipcode = #{zipcode}, 
					address1 = #{address1}, 
					address2 = #{address2} 
					where seq = ${seq}
  </update>
  
  
  
  </mapper>